
xkb_types "hatchak" {
    include "complete"
    
    virtual_modifiers Super;
    //virtual_modifiers LSuper;
    //virtual_modifiers RSuper;

    type "Hatchak_VT" {
        modifiers = Shift+LevelFive+Super+Control;
        map[None] = Level1;
        map[Shift] = Level2;
        map[LevelFive] = Level3;
        map[Shift+LevelFive] = Level4;
        map[LevelFive+Super] = Level5;
        preserve[Shift] = Shift;
        preserve[Shift+LevelFive] = Shift;
        level_name[Level1] = "Base";
        level_name[Level2] = "Shift";
        level_name[Level3] = "L5";
        level_name[Level4] = "Shift_L5";
        level_name[Level5] = "Super_L5";
    };
};

